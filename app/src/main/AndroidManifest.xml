<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.kaeawc.servicestartupapp" >

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".services.SuicidalService"
            android:enabled="true"
            android:exported="true" >
        </service>
        <service
            android:name=".services.MurderousService"
            android:enabled="true"
            android:exported="true" >
        </service>
        <service
            android:name=".services.FastStartingService"
            android:enabled="true"
            android:exported="true" >
        </service>
        <service
            android:name=".services.PubnubService"
            android:enabled="true"
            android:exported="true" >
        </service>
        <service
            android:name=".services.SlowStartingService"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest>
